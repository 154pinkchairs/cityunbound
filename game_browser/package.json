    "start": "npm run build-rust && parcel watch index.html --no-hmr --log-level 3"
  },
  "dependencies": {
    "antd": "^3.8.1",
    "gl-matrix": "^2.6.1",
    "immutability-helper": "^2.7.1",
    "monet": "git+https://github.com/aeickhoff/monet.git",
    "mousetrap": "^1.6.2",
    "msgpack-lite": "^0.1.26",
    "parcel-bundler": "^1.9.7",
    "react": "^16.4.1",
    "react-addons-update": "^15.6.2",
    "react-container-dimensions": "^1.3.4",
    "react-dom": "^16.4.1",
    "stacktrace-js": "^2.0.0"
  },
  "devDependencies": {
    "copyfiles": "^2.1.0",
    "less": "^3.8.1"
  },
  "less": {
    "javascriptEnabled": true
  },
  "browser": {
    "fs": false
  }
